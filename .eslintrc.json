{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"

    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["off", "double"],
        "no-undef": "off",
        "no-unused-vars":"off"
    }
}
